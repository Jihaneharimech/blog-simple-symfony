
{% extends 'base.html.twig' %}

{% block title %}Créer un nouvel article{% endblock %}

{% block body %}
    <h1>Créer un nouvel article</h1>

    {{ form_start(form) }}
    {{ form_row(form.titre) }}
    {{ form_row(form.contenu) }}
    <button type="submit" class="btn btn-primary">Créer l'article</button>
    {{ form_end(form) }}
{% endblock %}
